/*每个页面公共css */
view, cover-view, scroll-view {
  box-sizing: border-box;
}
button {
  margin: 0;
  padding: 0;
  border: none !important;
  background-color: #fff;
}
button::after {
  border: none;
}
.btnHoverClass {
  background-color: #FFFFFF !important;
}
._sup {
  font-size: 10rpx;
  vertical-align: text-top;
}
._sub {
  font-size: 10rpx;
  vertical-align: text-bottom;
}
scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
.singleRowText {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.multipleRowText {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

